<Window x:Class="WpfPriemgetal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPriemgetal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnControleer" Content="Controleer" HorizontalAlignment="Left" Height="36" Margin="146,194,0,0" VerticalAlignment="Top" Width="234" FontSize="16" Click="btnControleer_Click"/>
        <Label x:Name="lblTextControleer" Content="Label" HorizontalAlignment="Left" Height="40" Margin="146,255,0,0" VerticalAlignment="Top" Width="234" FontSize="16" FontWeight="Bold" FontStyle="Italic"/>
        <TextBox x:Name="txtGetal" HorizontalAlignment="Left" Height="23" Margin="260,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Label x:Name="lblGetal" Content="getal" HorizontalAlignment="Left" Height="37" Margin="164,152,0,0" VerticalAlignment="Top" Width="64" FontSize="16"/>
        <Label x:Name="lblTitel" Content="Priemgetal" HorizontalAlignment="Left" Height="35" Margin="260,79,0,0" VerticalAlignment="Top" Width="98" FontSize="16"/>

    </Grid>
</Window>
